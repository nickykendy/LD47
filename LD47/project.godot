; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node2D",
"class": "Level",
"language": "GDScript",
"path": "res://Scripts/Level.gd"
}, {
"base": "Area2D",
"class": "Plot",
"language": "GDScript",
"path": "res://Scripts/Plot.gd"
} ]
_global_script_class_icons={
"Level": "",
"Plot": ""
}

[application]

config/name="Ld 47"
run/main_scene="res://Levels/Start.tscn"
config/icon="res://icon.png"

[display]

window/size/width=480
window/size/height=320
window/size/test_width=1440
window/size/test_height=960
window/stretch/mode="2d"
window/stretch/aspect="keep"

[rendering]

quality/intended_usage/framebuffer_allocation=0
quality/intended_usage/framebuffer_allocation.mobile=0
quality/2d/use_pixel_snap=true
environment/default_environment="res://Settings/default_env.tres"
